package Array1;
import java.util.*;

public class ArrangeNumbersInArray {
	
	public static int[] inputArray() {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int arr[]=new int[n];
		for(int i=0;i<n;i++) {
			arr[i]=sc.nextInt();
		}
		sc.close();
		return arr;
	}
	
	public static void main(String args[]) {
		
		
		
	}
}
